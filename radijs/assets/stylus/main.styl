@import 'reset.styl'

html,
body
  padding 0
  margin 0
  font-size 15px

.title
  font-family 'Roboto', sans-serif
  font-size 61px
  color #91AFFF
  font-weight 300

.historyContainer
  margin 2%

.historyComponent
  background #FFFFFF 0% 0% no-repeat padding-box
  box-shadow 0px 0px 15px #00000029
  border-radius 8px
  height 300px
  width 100%
  margin 2.5% 0

.historyPadding
  padding 5%
